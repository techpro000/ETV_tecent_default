<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="up_video_view">
        <attr name="render_with_texture" format="boolean" />
    </declare-styleable>

    <declare-styleable name="MarqueeView">
        <attr name="textcolor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="isRepeat" format="boolean" />
        <attr name="speed" format="integer" />
        <attr name="startPoint" format="integer">
            <enum name="start" value="0" />
            <enum name="end" value="1" />
        </attr>
        <attr name="direction" format="integer">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>


    <!--设置界面设置选项-->
    <declare-styleable name="Setting_Button">
        <attr name="img_left" format="reference" />
        <attr name="txt_title" format="string" />
        <attr name="txt_content" format="string" />
        <attr name="btn_txt_right" format="string" />
    </declare-styleable>

    <declare-styleable name="RopeProgressBar">
        <attr name="ropeMax" format="integer" />
        <attr name="ropeProgress" format="integer" />
        <attr name="ropePrimaryColor" format="color" />
        <attr name="ropeSecondaryColor" format="color" />
        <attr name="ropeSlack" format="dimension" />
        <attr name="ropeStrokeWidth" format="dimension" />
        <attr name="ropeDynamicLayout" format="boolean" />
    </declare-styleable>

    <declare-styleable name="MyProgressView">
        <attr name="progresscolor" format="color" />
        <attr name="progressbg" format="color" />
    </declare-styleable>




    <declare-styleable name="Banner">
        <!--<attr name="delay_time" format="integer" />-->
        <!--<attr name="layout_id" format="reference" />-->
    </declare-styleable>

    <attr name="PermissionBackround" format="reference"></attr>
    <attr name="PermissionTitleColor" format="color"></attr>
    <attr name="PermissionMsgColor" format="color"></attr>
    <attr name="PermissionItemTextColor" format="color"></attr>
    <attr name="PermissionButtonTextColor" format="color"></attr>
    <attr name="PermissionButtonBackground" format="reference"></attr>
    <attr name="PermissionBgFilterColor" format="color"></attr>
    <attr name="PermissionIconFilterColor" format="color"></attr>

</resources>